				},
				{
					"color": "#767676",
					"name": "Contrast Two",
					"slug": "contrast-two"
				},
				{
					"color": "#D2D3D4",
					"name": "Contrast Three",
					"slug": "contrast-three"
				},
				{
					"color": "#006DB7",
					"name": "Accent One",
					"slug": "accent-one"
				},
				{
					"color": "#FFCF00",
