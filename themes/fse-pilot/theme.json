		"typography": {
			"defaultFontSizes": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "\"Barlow Condensed\"",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/barlow-condensed/barlow-condensed-700-normal.woff2"
							]
						}
					],
					"fontFamily": "\"Barlow Condensed\", sans-serif",
					"name": "Barlow Condensed",
					"slug": "barlow-condensed"
				},
				{
					"fontFace": [
						{
							"fontFamily": "\"Plus Jakarta Sans\"",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/plus-jakarta-sans/plus-jakarta-sans-400-normal.woff2"
							]
						},
						{
							"fontFamily": "\"Plus Jakarta Sans\"",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/plus-jakarta-sans/plus-jakarta-sans-500-normal.woff2"
							]
						},
						{
							"fontFamily": "\"Plus Jakarta Sans\"",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/plus-jakarta-sans/plus-jakarta-sans-700-normal.woff2"
							]
						},
						{
							"fontFamily": "\"Plus Jakarta Sans\"",
