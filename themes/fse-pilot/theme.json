							]
						},
						{
							"fontFamily": "\"Plus Jakarta Sans\"",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/plus-jakarta-sans/plus-jakarta-sans-700-italic.woff2"
							]
						},
						{
							"fontFamily": "\"Plus Jakarta Sans\"",
							"fontStyle": "italic",
							"fontWeight": "800",
							"src": [
								"file:./assets/fonts/plus-jakarta-sans/plus-jakarta-sans-800-italic.woff2"
							]
						}
					],
					"fontFamily": "\"Plus Jakarta Sans\", sans-serif",
					"name": "Plus Jakarta Sans",
					"slug": "plus-jakarta-sans"
				}
			],
			"fontSizes": [
				{
					"name": "Tiny",
					"size": "clamp(12px, 0.190vw + 11.26px, 14px)",
					"slug": "tiny"
				},
				{
					"name": "Small",
					"size": "clamp(14px, 0.190vw + 13.26px, 16px)",
					"slug": "small"
				},
				{
					"name": "Normal",
					"size": "clamp(16px, 0.190vw + 15.26px, 18px)",
					"slug": "normal"
				},
				{
					"name": "Large",
					"size": "clamp(18px, 0.286vw + 16.89px, 21px)",
					"slug": "large"
				},
				{
					"name": "Extra-Large",
					"size": "clamp(21px, 0.286vw + 19.89px, 24px)",
					"slug": "x-large"
				},
				{
					"name": "Huge",
					"size": "clamp(24px, 0.762vw + 21.03px, 32px)",
					"slug": "huge"
				}
			],
			"fontStyle": true,
			"fontWeight": true,
			"letterSpacing": true,
			"textTransform": true
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/button": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--plus-jakarta-sans)",
					"fontSize": "var(--wp--preset--font-size--normal)",
					"fontWeight": "500"
				}
			},
			"core/pullquote": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--plus-jakarta-sans)",
					"fontSize": "var(--wp--preset--font-size--extra-large)",
					"fontWeight": "700"
				}
			},
			"core/quote": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--plus-jakarta-sans)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontStyle": "italic",
					"fontWeight": "500"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base-one)",
			"text": "var(--wp--preset--color--contrast-one)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "clamp(56px, 3.81vw + 41.14px, 96px)",
					"letterSpacing": "-0.02em",
					"lineHeight": "0.9"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "clamp(40px, 0.762vw + 37.03px, 48px)",
					"letterSpacing": "-0.01em",
					"lineHeight": "1"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "clamp(32px, 0.381vw + 30.51px, 36px)",
					"letterSpacing": "-0.01em",
					"lineHeight": "1"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--plus-jakarta-sans)",
					"fontSize": "var(--wp--preset--font-size--extra-large)",
					"fontWeight": "800",
					"textTransform": "none"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--plus-jakarta-sans)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "800",
					"textTransform": "none"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--plus-jakarta-sans)",
					"fontSize": "var(--wp--preset--font-size--normal)",
					"fontWeight": "700",
					"textTransform": "none"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--barlow-condensed)",
					"fontWeight": "700",
					"textTransform": "uppercase"
				}
			},
			"link": {
				":hover": {
					"typography": {
						"textDecoration": "underline"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--contrast-one)"
				},
				"typography": {
					"textDecoration": "none"
				}
			}
		},
		"spacing": {
			"blockGap": "none",
			"padding": {
				"left": "var(--wp--preset--spacing--margins)",
				"right": "var(--wp--preset--spacing--margins)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--plus-jakarta-sans)",
			"fontSize": "var(--wp--preset--font-size--normal)",
			"fontWeight": "400",
			"lineHeight": "1.5"
		}
	}
}
