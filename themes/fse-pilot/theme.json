				{
					"name": "size-8",
					"size": "clamp(48px, 1.524vw + 42.057px, 64px)",
					"slug": "80"
				},
				{
					"name": "size-9",
					"size": "clamp(56px, 2.286vw + 47.086px, 80px)",
