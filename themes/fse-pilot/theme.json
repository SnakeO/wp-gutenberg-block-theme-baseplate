				}
			]
		},
		"custom": {
			"colorAlpha": {
				"goldGlow": "rgba(255, 207, 0, 0.5)"
			},
			"iconSize": {
				"minifigure": "clamp(82px, 2.286vw + 73.086px, 106px)"
			}
		},
		"dimensions": {
			"aspectRatios": [
				{
					"name": "Featured Image",
					"ratio": "2/1",
					"slug": "featured-image"
				},
				{
					"name": "Sidebar Image",
					"ratio": "176/141",
					"slug": "sidebar-image"
				},
				{
					"name": "Update Card",
					"ratio": "376/180",
					"slug": "update-card"
				}
			],
			"defaultAspectRatios": true
		},
		"layout": {
			"contentSize": "640px",
			"wideSize": "1216px"
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "margins",
